/*------------------SLIDER------------------------*/

@media (max-width: 1024px){
  .flickity-page-dots {
    display: none !important;
  }
}

.flickity-page-dots {
	counter-reset: a;

}

.flickity-page-dots .dot::after {
	content: counter(a);
	counter-increment: a;
}




.flickity-page-dots {
    display: flex;
    flex-direction: column;
    height: 100%;
    width: 50px;
    top: 0 !important;
    justify-content: center;
    left: rem-calc(125); 

}


.flickity-page-dots::before {
    content: "";
    height: 50%;
    width: 2px;
    border-right: 1px solid #ccc;
    display: block;
    margin-left: 5px;

}


.flickity-page-dots::after {
    content: "";
    height: 50%;
    width: 2px;
    border-right: 1px solid #ccc;
    display: block;
    margin-left: 5px;

}


.flickity-page-dots .dot {
    width: 40px;
    height: 40px;
    font-size: 15px;
    font-weight: bold;
    display: flex;
    align-items: center;
    justify-content: center;
    margin: 0 0 2px -14px;
    padding: 3px 3px;
    background-color: transparent;
    color: white;
  position: relative;
}


.flickity-page-dots li:last-child  {
   margin-bottom: 5px !important;
}

.flickity-page-dots li:first-child  {
   margin-top: 5px !important;
}


.flickity-page-dots .is-selected {
  
    width: 40px;
    height: 48px;
    color: $blue;
    position: relative;
  
  }


  


  



